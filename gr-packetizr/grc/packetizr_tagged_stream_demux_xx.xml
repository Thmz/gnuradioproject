<?xml version="1.0"?>
<block>
  <name>Tagged Stream Demux</name>
  <key>packetizr_tagged_stream_demux_xx</key>
  <category>packetizr</category>
  <import>import packetizr</import>
  <make>packetizr.tagged_stream_demux_xx($type.size*$vlen, $lengthtagname, $splitsize)</make>
  
  <param>
    <name>IO Type</name>
    <key>type</key>
    <type>enum</type>
    <option>
      <name>Complex</name>
      <key>complex</key>
      <opt>size:gr.sizeof_gr_complex</opt>
    </option>
    <option>
      <name>Float</name>
      <key>float</key>
      <opt>size:gr.sizeof_float</opt>
    </option>
    <option>
      <name>Int</name>
      <key>int</key>
      <opt>size:gr.sizeof_int</opt>
    </option>
    <option>
      <name>Short</name>
      <key>short</key>
      <opt>size:gr.sizeof_short</opt>
    </option>
    <option>
      <name>Byte</name>
      <key>byte</key>
      <opt>size:gr.sizeof_char</opt>
    </option>
  </param>
  <param>
    <name>Length tag names</name>
    <key>lengthtagname</key>
    <value>"packet_len"</value>
    <type>string</type>
  </param>
  <param>
    <name>Number of outputs</name>
    <key>noutputs</key>
    <value>3</value>
    <type>int</type>
  </param>
  <param>
    <name>Vector Length</name>
    <key>vlen</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Split size</name>
    <key>splitsize</key>
    <value>(2,2)</value>
    <type>int_vector</type>
  </param>



 

   <sink>
    <name>in</name>
    <type>$type</type>
    <vlen>$vlen</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>$type</type>
    <vlen>$vlen</vlen>
    <nports>$noutputs</nports>
  </source>


</block>
