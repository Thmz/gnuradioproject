<block>
  <name>Modulate Vector 2</name>
  <key>variable_modulate_vector2</key>
  <category>Packetizr</category>
  <import>import packetizr</import>
  <!--<import>from gnuradio import digital</import>-->
  <var_make>self.$(id) = $(id) = packetizr.modulate_vector_cc($mod .to_basic_block(), $data)</var_make>
  <var_value>packetizr.modulate_vector_cc($mod .to_basic_block(), $data)</var_value>
  <make></make>

  <param>
    <name>Modulator</name>
    <key>mod</key>
    <value></value>
    <type>raw</type>
  </param>
  <param>
    <name>Data vector</name>
    <key>data</key>
    <value></value>
    <type>raw</type>
  </param>
</block>
